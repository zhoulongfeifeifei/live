page{
    padding: 0; 
    margin: 0;
    box-sizing: border-box;  
    -webkit-tap-highlight-color: transparent; 
    -webkit-appearance: none;  
    user-select: none;  
    -webkit-touch-callout: none;
}
view{
    box-sizing: border-box;  
    -webkit-tap-highlight-color: transparent;  
    -webkit-appearance: none;    
    user-select: none;  
    -webkit-touch-callout: none;
}
page{
    font-size:28rpx;
    font-family: Helvetica Neue,Helvetica,sans-serif;
    color: #e0e0e0;
    -webkit-font-smoothing: antialiased;
}
page{
    min-height: 100%;
    background-color: #f1f1f1;
}
input,textarea,button{
    outline: none !important;
}
button{
    border:0;
}
@font-face {
    font-family: 'iconfont';  /* project id 434260 */
    src: url('//at.alicdn.com/t/font_434260_j8uljm1gjf758kt9.eot');
    src: url('//at.alicdn.com/t/font_434260_j8uljm1gjf758kt9.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_434260_j8uljm1gjf758kt9.woff') format('woff'),
    url('//at.alicdn.com/t/font_434260_j8uljm1gjf758kt9.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_434260_j8uljm1gjf758kt9.svg#iconfont') format('svg');
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:24rpx;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
    overflow: hidden;
}
.loading{
    width: 100%;
    height: 100rpx;
    text-align: center;
    font-size: 24rpx;
    line-height: 50rpx;
    margin: 80rpx 0;
}
.loading .iconfont{
    font-size: 80rpx;
}
.loading .icon-jiazai:before {
    content: "\e62c";
}

.empty_box{
    width: 100%;
    padding: 100rpx;
    text-align: center;
}
.empty_box .iconfont{
    font-size: 126rpx;
}
.empty_box .icon-wodexingqiuweixuanzhong:before {
    content: "\e62c";
}
.empty_box .text{
    font-size: 24rpx;
    line-height: 40rpx;
}

